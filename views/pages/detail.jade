extends ../layout
block content
    .container
        .row
            .col-md-7
                embed(src="#{movie.flash}",allowfullScreen="true",quality="high",width="720",height="600",align="middle",type="application/x-shckwave-flash")
            .col-md-5
                dl.dl-horizontal
                    dt 电影名
                    dd= movie.title
                    dt 导演
                    dd= movie.doctor
                    dt 国家
                    dd= movie.country
                    dt 语言
                    dd= movie.language
                    dt 简介
                    dd= movie.summary


